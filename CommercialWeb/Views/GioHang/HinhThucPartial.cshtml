@using CommercialWeb.Models
@model IEnumerable<HinhThucGiaoHang>
<!--Dropdown hinh thuc start-->
<select id="dropdown-hinhthuc" class="custom-select" required>
    <option value="">Hình thức thanh toán</option>
    @foreach (var item in Model)
    {
        <option value= @item.CuocPhi.ToString()>@item.TenHinhThuc</option>
    }
</select>
<!--Dropdown hinh thuc end-->
